{
  "name": "mui-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run generate-i18n-types && npm run copy-translations && concurrently \"npm run watch-translations\" \"vite\"",
    "build": "tsc -b && vite build && npm run copy-translations",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "copy-translations": "cpx \"src/i18n/translations/**/*\" public/i18n/translations",
    "watch-translations": "cpx \"src/i18n/translations/**/*\" public/i18n/translations --watch",
    "generate-i18n-types": "i18next-resources-for-ts interface -i ./src/i18n/translations -o ./src/i18n/resources.d.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.1.9",
    "@mui/x-data-grid": "^7.23.1",
    "@reduxjs/toolkit": "^2.4.0",
    "i18next": "^24.0.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.3",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.0.2",
    "react-use": "^17.6.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@faker-js/faker": "^9.2.0",
    "@types/json-server": "^0.14.7",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.0",
    "cpx": "^1.5.0",
    "eslint": "^9.13.0",
    "eslint-plugin-perfectionist": "^4.3.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.11.0",
    "i18next-resources-for-ts": "^1.5.0",
    "json-server": "^1.0.0-beta.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9"
  }
}
